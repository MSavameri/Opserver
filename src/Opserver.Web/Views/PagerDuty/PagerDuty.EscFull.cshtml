@model List<OnCall>
@{
    this.SetPageTitle("PagerDuty Full Escalation");
}
<h4 class="modal-title">
    Full Escalation Schedule
</h4>
<table class="table table-striped table-hover table-super-condensed table-responsive text-nowrap">
    <thead>
        <tr>
            <th>Name</th>
            <th>Level</th>
            <th>Policy</th>
            <th>Email</th>
            <th>Phone</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var a in Model.OrderBy(u => u.EscalationLevel))
        {
            <partial name="PagerDuty.OnCallRow" model="a" />
        }
    </tbody>
</table>

