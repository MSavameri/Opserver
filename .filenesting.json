{
  "help": "https://go.microsoft.com/fwlink/?linkid=866610",
  "root": true,

  "dependentFileProviders": {
    "add": {
      "pathSegment": {
        "add": {
          "ExtensionMethods.*": [ ".cs" ],
          "Models\\*.*": [ "*.cs" ]
        }
      },
      "fileSuffixToExtension": {
        "add": {
          ".Model.cs": [ ".cshtml" ],
          ".json.defaults": [ ".json" ],
          ".min.css": [ ".css" ],
          ".min.js": [ ".js" ],
          ".min.js.map": [ ".min.js" ]
        }
      }
    }
  }
}